void oledStateChange(uint8_t lcdStatus){
  if(prevlcdStatus != lcdStatus){
    switch (lcdStatus)
    {
      case 0:
        oled_drawSplashScreen();
        break;
      case 1:
        oled_drawIdleScreen();
        break;
      case 2:
        oled_drawSdInitError();
        break;
      case 3:
        oled_drawSdFull();
        break;
      case 4:
        oled_drawHex2Txt();
        break;
      case 5:
        oled_drawTxt2Bmp();
        break;
      case 6:
        oled_drawTxt2Png();
        break;
      case 7:
        oled_drawForceNewFile();
        break;
      default:
        display.clearDisplay();
        break;
    }
    prevlcdStatus = lcdStatus;
  }
}

